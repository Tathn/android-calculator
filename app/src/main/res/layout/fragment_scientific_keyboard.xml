<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scientific_calculator_frame"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.patryk.calculator.ScientificKeyboardFragment"
    android:background="#A9C5EB">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto">

        <android.support.constraint.Guideline
            android:id="@+id/guideline25"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.25"
            tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="96dp" />
        <android.support.constraint.Guideline
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/guideline50"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5"
            tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="192dp" />
        <android.support.constraint.Guideline
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/guideline75"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.75"
            tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="288dp" />

        <android.support.constraint.Guideline
            android:id="@+id/guidelineh22"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0"
            tools:layout_editor_absoluteY="113dp"
            tools:layout_editor_absoluteX="0dp" />

        <android.support.constraint.Guideline
            android:id="@+id/guidelineh38"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.33"
            tools:layout_editor_absoluteY="194dp"
            tools:layout_editor_absoluteX="0dp" />

        <android.support.constraint.Guideline
            android:id="@+id/guidelineh54"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.66"
            tools:layout_editor_absoluteY="276dp"
            tools:layout_editor_absoluteX="0dp" />

        <android.support.constraint.Guideline
            android:id="@+id/guidelineh70"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.99"
            tools:layout_editor_absoluteY="358dp"
            tools:layout_editor_absoluteX="0dp" />

        <android.support.constraint.Guideline
            android:id="@+id/guidelineh85"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.8"
            tools:layout_editor_absoluteY="435dp"
            tools:layout_editor_absoluteX="0dp" />

        <Button
            android:id="@+id/buttonE"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="E"
            android:textSize="23sp"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh38"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@+id/guideline25"
            app:layout_constraintTop_toTopOf="@+id/guidelineh22"
            app:layout_constraintVertical_bias="0.0"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>

        <Button
            android:id="@+id/buttonFactorial"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="!"
            android:textSize="23sp"
            app:layout_constraintRight_toLeftOf="@+id/guideline75"
            app:layout_constraintTop_toTopOf="@+id/guidelineh22"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh38"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintLeft_toLeftOf="@+id/guideline50"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>

        <Button
            android:id="@+id/buttonLn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="LN"
            android:textSize="23sp"
            app:layout_constraintRight_toLeftOf="@+id/guideline75"
            app:layout_constraintTop_toTopOf="@+id/guidelineh38"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh54"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/guideline50"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>

        <Button
            android:id="@+id/buttonTan"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="TAN"
            android:textSize="23sp"
            app:layout_constraintRight_toLeftOf="@+id/guideline75"
            app:layout_constraintTop_toTopOf="@+id/guidelineh54"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh70"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/guideline50"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>

        <Button
            android:id="@+id/buttonPI"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="PI"
            android:textSize="23sp"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh38"
            app:layout_constraintRight_toLeftOf="@+id/guideline50"
            app:layout_constraintTop_toTopOf="@+id/guidelineh22"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/guideline25"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>

        <Button
            android:id="@+id/buttonToRad"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="RAD"
            android:textSize="23sp"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh54"
            app:layout_constraintRight_toLeftOf="@+id/guideline50"
            app:layout_constraintTop_toTopOf="@+id/guidelineh38"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/guideline25"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>

        <Button
            android:id="@+id/buttonCos"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="COS"
            android:textSize="23sp"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh70"
            app:layout_constraintRight_toLeftOf="@+id/guideline50"
            app:layout_constraintTop_toTopOf="@+id/guidelineh54"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/guideline25"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>

        <Button
            android:id="@+id/buttonPercent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="%"
            android:textSize="23sp"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh38"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guidelineh22"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="1.0"
            app:layout_constraintLeft_toLeftOf="@+id/guideline75"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>

        <Button
            android:id="@+id/buttonLog10"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="LOG10"
            android:textSize="23sp"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh54"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guidelineh38"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/guideline75"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>

        <Button
            android:id="@+id/buttonEToX"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="E^X"
            android:textSize="23sp"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh70"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guidelineh54"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintLeft_toLeftOf="@+id/guideline75"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>


        <Button
            android:id="@+id/buttonToDeg"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="DEG"
            android:textSize="23sp"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh54"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guidelineh38"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintRight_toLeftOf="@+id/guideline25"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>

        <Button
            android:id="@+id/buttonSin"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:text="SIN"
            android:textSize="23sp"
            app:layout_constraintBottom_toTopOf="@+id/guidelineh70"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guidelineh54"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0"
            app:layout_constraintRight_toLeftOf="@+id/guideline25"
            android:background="#A9C5EB"
            android:onClick="onButtonClick"/>
    </android.support.constraint.ConstraintLayout>

</FrameLayout>
